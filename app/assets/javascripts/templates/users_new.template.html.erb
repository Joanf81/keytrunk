<div class="row">
  <div class="col-xs-1"></div>
  <div class="col-xs-10">
    <br>
    <div class="row">
      <div class="col-xs-12">
        <center>
          <h1>New user form</h1>
        </center>
      </div>
    </div>
    <br>
    <br>
    <form name="formUserRegister" class="form-register" role="register">
      <div class="form-group">
        <div class="row">
          <div class="col-xs-6">
            <input type="text" id="email" name="email" class="form-control" placeholder="Insert e-mail" ng-model="email" required>
          </div>
          <div class="col-xs-6">
            <input type="text" id="username" name="username" class="form-control " placeholder="Insert user name" ng-model="username" required>
          </div>
        </div>

        <div class="row">
          <div class="col-xs-6">
            <input type="password" id="pass" name="pass" class="form-control" placeholder="Password" ng-model="pass" required>
          </div>
          <div class="col-xs-6">
            <input type="password" id="pass_repeat" name="pass_repeat" class="form-control" placeholder="Repeat password" ng-model="pass_repeat" required>
          </div>
        </div>


        <div class="row">
          <div class="col-xs-12">
            <center>
              <button class="btn btn-default" type="button" ng-click='users_create()' ng-href='#here'>Register</button>
            </center>
          </div>
        </div>

      </div>
    </form>
  </div>
  <div class="col-xs-1"></div>
</div>
<br><br><br>